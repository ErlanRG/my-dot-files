[DISK]
command=df -h / | awk '/\//{ printf(" ğŸ’¾ %4s/%s \n", $4, $2)  }'
interval=2
background="#98971a"
color="#282828"

[MEM]
command=free -h | awk '/Mem:/ { printf(" ğŸ %5s/%s \n", $3, $2)  }'
interval=1
background="#98971a"
color="#282828"

[TIME_DATE]
command=date +" %a, %d %b - %H:%M:%S"
interval=1
background="#98971a"
color="#282828"
